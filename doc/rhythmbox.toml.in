[library]
version = "@VERSION@"
description = "Programming interfaces available to Rhythmbox plugins."
authors = "Rhythmbox contributors"
license = "GPL-2.0-or-later"
browse_url = "https://gitlab.gnome.org/GNOME/rhythmbox"
repository_url = "https://gitlab.gnome.org/GNOME/rhythmbox.git"
logo_url = "../org.gnome.Rhythmbox3.svg"
website_url = "https://gnome.pages.gitlab.gnome.org/rhythmbox/"
dependencies = [
  "Gio-2.0",
  "GObject-2.0",
  "Gst-1.0",
  "Gtk-3.0",
]
devhelp = false
search_index = true

[dependencies."GObject-2.0"]
name = "GObject"
description = "The base type system library"
docs_url = "https://developer.gnome.org/gobject/stable"

[dependencies."Gio-2.0"]
name = "Gio"
description = "GLib I/O library"
docs_url = "https://developer.gnome.org/gio/stable"

[dependencies."Gst-1.0"]
name = "Gst"
description = "GStreamer media framework"
docs_url = "https://gstreamer.freedesktop.org/documentation/libs.html"

[dependencies."Gtk-3.0"]
name = "Gtk"
description = "The GTK toolkit"
docs_url = "https://docs.gtk.org/gtk3/"

[theme]
name = "basic"
show_index_summary = true
show_class_hierarchy = true

[source-location]
base_url = "https://gitlab.gnome.org/GNOME/rhythmbox/-/blob/main/"
file_format = "{filename}#L{line}"

[extra]
urlmap_file = "urlmap.js"
content_files = [
  "internals.md",
  "plugin-guide.md",
  "plugin-examples.md"
]
