      </schema>
      <schema>
        <key>/schemas/apps/rhythmbox/plugins/rblirc/active</key>
        <applyto>/apps/rhythmbox/plugins/rblirc/active</applyto>
        <owner>rhythmbox</owner>
        <type>bool</type>
        <default>FALSE</default>
        <locale name="C">
	<short>True if the LIRC plugin is enabled.</short>
